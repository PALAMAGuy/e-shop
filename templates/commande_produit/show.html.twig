{% extends 'base.html.twig' %}

{% block title %}CommandeProduit{% endblock %}

{% block body %}
    <h1>CommandeProduit</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ commande_produit.id }}</td>
            </tr>
            <tr>
                <th>Quantity</th>
                <td>{{ commande_produit.quantity }}</td>
            </tr>
            <tr>
                <th>Frais_port</th>
                <td>{{ commande_produit.fraisPort }}</td>
            </tr>
            <tr>
                <th>Assurance</th>
                <td>{{ commande_produit.assurance ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>Tva</th>
                <td>{{ commande_produit.tva }}</td>
            </tr>
            <tr>
                <th>Prix</th>
                <td>{{ commande_produit.prix }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_commande_produit_index') }}">back to list</a>

    <a href="{{ path('app_commande_produit_edit', {'id': commande_produit.id}) }}">edit</a>

    {{ include('commande_produit/_delete_form.html.twig') }}
{% endblock %}
